<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="succWater.Views.AddPlantView">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="plantName" Placeholder="Plant Name" ></Entry>
            <Entry x:Name="plantSpecies" Placeholder="Species"></Entry>
            <Button Clicked="AddPlant"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>